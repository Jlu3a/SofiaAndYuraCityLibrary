<Window x:Class="CityLibrary.Windows.PopularityBooksWindow1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CityLibrary.Windows"
        mc:Ignorable="d"
        Title="PopularityBooksWindow1" Height="450" Width="800"
        Loaded="PopularityBooksWindow1_Loaded">
    
    <Grid>
        <DataGrid x:Name="BooksPopularityDataGrid" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Название книги" Binding="{Binding BookName}" />
                <DataGridTextColumn Header="Количество прочтений" Binding="{Binding Count}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
